.map_container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}
.map_container .map {
  width: 100vw;
  height: 100vh;
}
.map_container .center_view {
  position: relative;
  top: 50vh;
  left: 50vw;
  width: 60rpx;
  height: 60rpx;
  margin: -30rpx 0 0 -30rpx;
}
.map_container .center {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
